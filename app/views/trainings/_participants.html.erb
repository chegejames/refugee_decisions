<ul class="breadcrumb">
        <li><%= link_to "#{@training.name} held at: #{@location + " Date: " +@start_date}", training_training_sessions_path(@training)%> <span class="divider">/</span></li>
    <li class="active">Participants</li>
</ul>
<%if @judges.empty?%>
<span class="label label-warning">sorry no judges found</span>
<%else%>
<span class="label label-success"><%= pluralize(@judges.count, 'Judge') %> found</span>
<%end%>
<table class="table table-striped table-bordered table-hover table-condensed">
  <tr>
    <th>Membership ID</th>
    <th>Name</th>
    <th>Deisgnation</th>
  </tr>
  <% @judges.each do | judge|%>
  <tr>
  <td><%= judge.membership_id%></th>
  <td><%= judge.designation%></th>
  <td><%= judge.name%></td>
  <td><%= link_to "view member", judge, class: "btn btn-primary btn-small"%></td>
  </tr>
  <%end%>
 </table>
