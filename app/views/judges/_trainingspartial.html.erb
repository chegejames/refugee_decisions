<ul class="breadcrumb">
         <li><%= link_to "#{@judge.designation + " "+@judge.name}".upcase!, @judge%> <span class="divider">/</span></li>
    <li class="active">Trainings</li>
</ul>
<%if @training_sessions.empty?%>
<span class="label label-warning">sorry no cases found</span>
<%else%>
<span class="label label-success"><%= pluralize(@training_sessions.count, 'training') %> found</span>
<%end%>
<table class="table table-striped table-bordered table-hover table-condensed">
  <tr>
  <th>Training</th>
  <th>Date of training</th>
  <th>Location</th>
  </tr>
  <%@training_sessions.each do |training_session|%>
   <tr>
     <td><%= training_session.training.name%></td>
     <td><%= training_session.date%></td>
     <td><%= training_session.location%></td>
     <td><%= link_to "training", training_session.training, class: "btn btn-primary btn-mini"%></td>
   </tr>
  <%end%>
</table>
<%= will_paginate @training_sessions%>
