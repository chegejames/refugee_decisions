<%set_meta_tags :nofollow => true%>
<%= form_for([@training, @training_session]) do |f| %>
  <% if @training_session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@training_session.errors.count, "error") %> prohibited this training_session from being saved:</h2>

      <ul>
      <% @training_session.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
   <div class="field">
    <%= f.label :start_date %><br />
    <%= f.text_field :start_date, class: "datepicker", required: true%>
  </div>
  <div class="field">
    <%= f.label :end_date %><br />
    <%= f.text_field :end_date, class: "datepicker", required: true%>
  </div>
  <div class="field">
     <%= f.label :location%><br/>
     <%= f.text_field :location, required: true%>
  </div>
  <div class="field">
    <%= f.label :judge_tokens, "Judges" %><br />
    <%= f.text_field :judge_tokens, id: "judges_tokens", required: true%>
  </div>

  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
